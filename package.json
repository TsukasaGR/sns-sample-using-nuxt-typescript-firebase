{
  "name": "nuxt-firebase-gcp-sample",
  "version": "1.0.0",
  "description": "Nuxt.js, firebase, GCPのサンプル",
  "author": "TsukasaGR",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "deploy": "yarn build && gcloud app deploy -q",
    "browse": "gcloud app browse",
    "log": "gcloud app logs tail -s default",
    "start": "nuxt start",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint:fix": "yarn lint --fix",
    "post-update": "yarn upgrade --latest",
    "clear:cache": "rimraf node_modules/.cache/hard-source/"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@firebase/app-types": "^0.4.4",
    "@firebase/firestore-types": "^1.5.0",
    "@nuxt/typescript": "^2.8.1",
    "@nuxtjs/pwa": "^3.0.0-beta.8",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vue/composition-api": "^0.3.2",
    "axios": "^0.21.1",
    "blueimp-load-image": "^2.24.0",
    "bulma-badge": "^3.0.1",
    "core-js": "^2.6.5",
    "dayjs": "^1.8.16",
    "dotenv": "^8.2.0",
    "firebase": "^7.0.0",
    "firebaseui": "^4.2.0",
    "grpc": "^1.24.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.0",
    "nuxt": "^2.6.1",
    "nuxt-buefy": "^0.3.15",
    "sass-loader": "^8.0.0",
    "ts-node": "^8.4.1",
    "vue-firestore": "^0.3.22",
    "vue-property-decorator": "^8.2.2",
    "vue-rollbar": "^1.0.0",
    "vuexfire": "^3.1.2"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.3.2",
    "@nuxt/typescript-runtime": "^0.2.2",
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-config-typescript": "^0.1.3",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/google-analytics": "^2.2.1",
    "@types/axios": "^0.14.0",
    "@typescript-eslint/parser": "^2.6.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-prettier": "^3.1.1",
    "firebase-tools": "^7.5.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0"
  }
}
